-use yii\helpers\Html
-use yii\helpers\Url
-use jacmoe\mdpages\helpers\Page

-$view->title = isset($page->title) ? $page->title : 'Untitled'

-
    foreach($view->params['breadcrumbs'] as $breadcrumb) {
        if(isset($breadcrumb['url'])) {
            echo Html::a($breadcrumb['label'], $breadcrumb['url']) . '  ';
        } else {
            echo $breadcrumb['label'] . '  ';
        }
    }
hr
.content
    !=$content
.content
    hr
    h3 Contributors to this page
    -
        foreach($page->contributors as $contributor) {
            echo Html::a(Html::img(Yii::getAlias('@web/avatars/') . $contributor->login . '.png', array('width' => '24', 'height' => '24', 'title' => $contributor->name)), $contributor->html_url);
        }
